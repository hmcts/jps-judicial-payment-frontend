{
  "name": "jps-judicial-payment-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:node": "cd api && yarn watch",
    "start:ng": "ng serve --port 3000 --host 0.0.0.0 --proxy-config proxy.config.json",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "node node_modules/@angular/cli/bin/ng build -c production",
    "watch": "ng build --watch --configuration development",
    "test:watch": "ng test",
    "test": "ng test --watch=false --browsers ChromeHeadless",
    "test:coverage": "ng test --code-coverage --source-map --watch=false --browsers ChromeHeadless",
    "test:a11y": "echo 'TODO'",
    "test:smoke": "echo 'TODO'",
    "test:functional": "HEADLESS=true codeceptjs run -c ./test/end-to-end/codecept.conf.js --grep @functional --steps --reporter mochawesome",
    "cichecks": "yarn && yarn build:ssr && ng lint && yarn test",
    "lint": "ng lint",
    "dev:ssr": "ng run jps-judicial-payment-frontend:serve-ssr",
    "serve:ssr": "node dist/jps-judicial-payment-frontend/server/main.js",
    "build:ssr": "yarn run ng build && yarn run ng run jps-judicial-payment-frontend:server",
    "prerender": "ng run jps-judicial-payment-frontend:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/platform-server": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@hmcts/properties-volume": "^0.0.14",
    "@hmcts/rpx-xui-node-lib": "^2.26.13",
    "@ngrx/store": "^15.4.0",
    "@nguniversal/express-engine": "^15.2.0",
    "config": "^3.3.9",
    "dotenv-extended": "^2.9.0",
    "express": "^4.15.2",
    "http-proxy-middleware": "^2.0.6",
    "mocha-multi": "^1.1.7",
    "mochawesome": "^7.1.3",
    "node-http-proxy-json": "^0.1.9",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "webdriverio": "^8.6.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
