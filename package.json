    "@angular/compiler-cli": "^15.2.0",
    "@codeceptjs/configure": "^0.10.0",
    "@codeceptjs/examples": "^1.2.1",
    "@codeceptjs/ui": "^0.5.0",
    "@nguniversal/builders": "^15.2.0",
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^14.15.0",
