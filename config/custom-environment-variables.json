{
    "environment": "NODE_CONFIG_ENV",
    "cookies": {
        "token": "COOKIE_TOKEN",
        "userId": "COOKIES_USER_ID"
    },
    "now": {
        "__name": "NOW",
        "__format": "json"
    },
    "services": {
        "idam": {
            "idamApiUrl": "SERVICES_IDAM_API_URL",
            "idamClientID": "SERVICES_IDAM_CLIENT_ID",
            "idamLoginUrl": "SERVICES_IDAM_LOGIN_URL",
            "oauthCallbackUrl": "SERVICES_IDAM_OAUTH_CALLBACK_URL",
            "iss": "SERVICES_IDAM_ISS_URL"
        },
        "s2s": "SERVICES_S2S"
    },
    "microservice": "MICROSERVICE",
    "sessionSecret": "SESSION_SECRET",
    "secrets": {
        "jps": {
          "mc-s2s-client-secret": "S2S_SECRET",
          "mc-idam-client-secret": "IDAM_SECRET",
          "webapp-redis-connection-string": "REDISCLOUD_URL",
          "system-user-name": "SYSTEM_USER_NAME",
          "system-user-password": "SYSTEM_USER_PASSWORD"
        }
    },
    "feature": {
        "secureCookieEnabled": {
            "__name": "FEATURE_SECURE_COOKIE_ENABLED",
            "__format": "json"
        },
        "redisEnabled": {
            "__name": "FEATURE_REDIS_ENABLED",
            "__format": "json"
        }
    },
    "redis": {
        "prefix": "REDIS_KEY_PREFIX",
        "ttl": "REDIS_TTL"
    }

}
